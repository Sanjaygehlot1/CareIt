{
  "name": "careit-tracker",
  "displayName": "CareIt Productivity Tracker",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*" 
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "careit.sendNow",
        "title": "CareIt: Force Send Analytics"
      }
    ],
    "configuration": {
      "title": "CareIt",
      "properties": {
        "careit.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your CareIt Personal API Key"
        },
        "careit.serverUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "The URL of the CareIt backend server"
        }
      }
    }
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}