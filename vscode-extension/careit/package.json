{
  "name": "careit-tracker",
  "displayName": "CareIt Productivity Tracker",
  "description": "Tracks your coding activity for the CareIt dashboard",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "careit.sendNow",
        "title": "CareIt: Force Send Analytics"
      }
    ],
    "configuration": {
      "title": "CareIt",
      "properties": {
        "careit.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your CareIt Personal API Key (Get it from http://localhost:5173/settings)"
        },
        "careit.serverUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "The URL of the CareIt backend server"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}